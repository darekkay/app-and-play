{
  "name": "app-and-play",
  "description": "Digital assistant for your board games",
  "version": "1.0.4",
  "author": "Darek Kay <hello@darekkay.com> (https://darekkay.com/)",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "boardgame.io": "0.35.1",
    "classnames": "2.2.6",
    "lodash": "4.17.15",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0"
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "node-sass": "4.13.0",
    "plop": "2.5.3",
    "prettier": "1.19.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://darekkay.com/app-and-play/",
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/thegame/"
    ]
  },
  "license": "MIT",
  "main": "src/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/darekkay/app-and-play.git"
  },
  "scripts": {
    "build": "react-scripts build",
    "ci": "npm run lint && npm run test",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,json,css,scss}\"",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "react-scripts start",
    "test": "react-scripts test --modulePaths=src --coverage"
  }
}
