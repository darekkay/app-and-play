{
  "name": "app-and-play",
  "description": "Board games assistant",
  "version": "1.0.0",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "boardgame.io": "0.25.5",
    "classnames": "2.2.6",
    "lodash": "4.17.11",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.4"
  },
  "devDependencies": {
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.10.0",
    "prettier": "1.14.3"
  },
  "homepage": "https://darekkay.com/app-and-play/",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "ci": "npm run lint:check && npm run test",
    "format": "prettier --write -- \"{src,public}/**/*.{js,jsx,json,css}\"",
    "lint:check": "eslint src/**/*.js",
    "lint:fix": "eslint --fix src/**/*.js",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom --modulePaths=src"
  }
}
